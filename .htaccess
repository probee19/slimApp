RewriteEngine On

RewriteCond %{SERVER_PORT} 80
RewriteRule ((?!\/(health/index.html)).*) https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]

#RedirectMatch ((?!\/(health/index.html)).*) https://%{HTTP_HOST}%{REQUEST_URI}

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^ index.php [QSA,L]