<!DOCTYPE html>
<html lang="fr">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">

    <title>{% block title %}{% endblock %} | Funizi</title>
    {% block graph %}{% endblock %}
    <!-- Bootstrap Core CSS -->
    <link href="{{ domain_url }}/src/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="{{ domain_url }}/src/css/3-col-portfolio.css" rel="stylesheet">
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-T8Gy5hrqNKT+hzMclPo118YTQO6cYprQmhrYwIiQ/3axmI1hQomh7Ud2hPOy8SP1" crossorigin="anonymous">
    <link rel="stylesheet" href="{{ domain_url }}/src/css/app.css?v=1.281">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

    <!-- Google Tag Manager -->
    <script>(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
    new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],
    j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
    'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
    })(window,document,'script','dataLayer','GTM-N4J4L86');</script>
    <!-- End Google Tag Manager -->

</head>
<body>
  <!-- Google Tag Manager (noscript) -->
<noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-N4J4L86"
height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
<!-- End Google Tag Manager (noscript) -->

<div id="fb-root"></div>
<script>(function(d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) return;
        js = d.createElement(s); js.id = id;
        js.src = "//connect.facebook.net/{{interface_ui.lang}}/sdk.js#xfbml=1&version=v2.10&appId=348809548888116";
        fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));
</script>
{% include 'templates/partials/menu.twig' %}
<!-- Page Content -->
<div data-domain="{{domain_url}}" data-resquest-uri="{{request_uri}}" id="domain"></div>
<div class="container">
    {% if not is_mobile() and not is_tablet() %}
    <div class="row text-center">
        <div class="fb-like" data-href="https://www.facebook.com/funizi/" data-layout="button_count" data-action="like" data-size="" data-show-faces="false" data-share="false"></div>
    </div>
    {% endif %}
    <div class="col-sm-12 text-center" style="margin-top:10px;">
      {% block content %}{% endblock %}
    </div>
    <!-- Modal FB Connexion -->
	<div id="facebook-login-modal" class="modal fade" role="dialog">
	    <div class="modal-dialog">

	        <div class="modal-content">
	            <div class="modal-header">
                    <img class="img-responsive" src="{{ base_url }}/creation-test/src/img/funizi.png" style="height:30px; margin: 0 auto;" alt="Funizi">
	                <button type="button" class="close" data-dismiss="modal">&times;</button>
	            </div>
	            <div class="modal-body text-center">
	                <p>
                  {{interface_ui.label_login}}
	                <br>
                  {{interface_ui.label_login_2}}
	                </p>
				</div>
	            <div class="row">
		            <div class="modal-footer text-center col-lg-offset-3 col-lg-6 col-xs-10 col-xs-offset-1" style="font-size:1em;">
						<button type="button"  id="btn_log_fb" onClick="" class="btn btn-lg btn-facebook btn-block " >
							<i class="fa fa-facebook-square" ></i>  &nbsp;{{interface_ui.btn_login}}
						</button>
					</div>
	            </div>
	        </div>
	    </div>
	</div>
    <!-- /Modal FB Connexion -->
</div>







  <!--Footer-->
  <footer class="footer">
      <!--Footer Links-->
      <div class="container-fluid text-center">
        <!--div class="footer-block text-center logo_footer">
            <a href="{{ domain_url }}?utm=from_logo"><img src="{{ base_url }}/creation-test/src/img/funizi.png" height="30"/></a>
        </div>
        <!--/.Footer Links-->
        <!--Copyright-->
        <div class="footer-block text-center">
            <a href="{{ path_for('privacypolicy') }}" class="link_footer">{{interface_ui.footer_privacy_policy}}</a>
            <!--<a href="#" class="link_footer">{{interface_ui.footer_about_us}}</a>-->
            <a href="https://docs.google.com/forms/d/e/1FAIpQLSfIXQZF0ZmdvjuvIM5bz5pomT-Ds6HH28ppkFR29bVksONofw/viewform" target="_blank" class="link_footer">{{interface_ui.footer_contact_us}}</a>
        </div>
      </div>
      <!--/.Copyright-->
  </footer>
  <!--/.Footer-->



<!-- /.container -->
<!-- jQuery -->
<script src="{{ domain_url }}/src/js/jquery.js"></script>

<!-- Bootstrap Core JavaScript -->
<script src="{{ domain_url }}/src/js/bootstrap.min.js"></script>
<script src="{{ domain_url }}/src/js/scripts.js?v=1.258"></script>
<script src="{{ domain_url }}/src/js/facebook-login.js?v=1.250"></script>

</body>
</html>
