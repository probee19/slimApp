<script src="{{ domain_url }}/src/js/jquery.js"></script>
<script class="init">
  $(document).ready(function () {
    tasks = Array('{{interface_ui.label_loading_1}}','{{interface_ui.label_loading_2}}','{{interface_ui.label_loading_3}}','{{interface_ui.label_loading_4}}','{{interface_ui.label_loading_5}}');
  });

</script>
