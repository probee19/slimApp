<body>
  <!-- Google Tag Manager (noscript) -->
<noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-N4J4L86"
height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
<!-- End Google Tag Manager (noscript) -->

<div id="fb-root"></div>
<script>(function(d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) return;
        js = d.createElement(s); js.id = id;
        js.src = "//connect.facebook.net/{{interface_ui.lang}}/sdk.js#xfbml=1&version=v2.10&appId=348809548888116";
        fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));
</script>
{% include 'templates/partials/menu.twig' %}
<!-- Page Content -->
<div data-domain="{{ domain_url }}" data-resquest-uri="{{ request_uri }}" data-defined-domain="{{ defined_base_domain }}" id="domain"></div>
<div class="container-fluid container-funizi">
    {% if not is_mobile() and not is_tablet() %}
    <div class="row text-center">
        <div class="fb-like" data-href="https://www.facebook.com/funizi/" data-layout="button_count" data-action="like" data-size="" data-show-faces="false" data-share="false"></div>
    </div>
    {% endif %}
    <!--<script async src="http://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    -->
    {% if no_ads %}
    {% else %}
      {% if not is_mobile() and not is_tablet() %}
        <div class="gallery text-center" style="margin:1em 0 1em;" >
            <!-- Funizi_728x90_Top_desktop -->
            <ins class="adsbygoogle"
                 style="display:inline-block;width:728px;height:90px"
                 data-ad-client="ca-pub-5139434732876009"
                 data-ad-slot="2957232538"></ins>
            <script>
                (adsbygoogle = window.adsbygoogle || []).push({});
            </script>
        </div>
      {% else %}
        <div style="margin:1em auto;" class="text-center">
            {% block banner_mobile_top %}{% endblock %}
        </div>
      {% endif %}
    {% endif %}

    <div class="col-sm-8 text-center gallery" style="">

        {% block content %}{% endblock %}

        {% if no_ads %}
        {% else %}
          <ins class="adsbygoogle"
               style="display:block;margin-bottom:5em;"
               data-ad-format="autorelaxed"
               data-ad-client="ca-pub-5139434732876009"
               data-ad-slot="3403735577"
               data-matched-content-ui-type="text_card"
               data-matched-content-rows-num="8"
               data-matched-content-columns-num="3"
          ></ins>
          <script>
              (adsbygoogle = window.adsbygoogle || []).push({});
          </script>
        {% endif %}
    </div>
    <!--<script async src="http://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    -->
    <div class="col-xs-12 col-sm-4 text-center aside-right" style="">
      <div style="position:fixed;  max-width:300px;">
        {% if no_ads %}
        {% else %}
          {% if not is_mobile() and not is_tablet() %}
            {% set banner = random(['banners/banner_300x250_bottom.twig']) %}
            {% include  banner %}

          {% endif %}
        {% endif %}
      </div>
    </div>

    <!-- Modal FB Connexion -->
	<div id="facebook-login-modal" class="modal fade" role="dialog">
	    <div class="modal-dialog">

	        <div class="modal-content">
	            <div class="modal-header">
                    <img class="img-responsive" src="{{ base_url }}/creation-test/src/img/funizi.png" style="height:30px; margin: 0 auto;" alt="Funizi">
	                <button type="button" class="close" data-dismiss="modal">&times;</button>
	            </div>
	            <div class="modal-body text-center">
	                <p>
                  {{interface_ui.label_login}}
	                <br>
                  {{interface_ui.label_login_2}}
	                </p>
				</div>
	            <div class="row">
		            <div class="modal-footer text-center col-lg-offset-3 col-lg-6 col-xs-10 col-xs-offset-1" style="font-size:1em;">
						<button type="button"  id="btn_log_fb" onClick="" class="btn btn-lg btn-facebook btn-block " >
							<i class="fa fa-facebook-square" ></i>  &nbsp;{{interface_ui.btn_login}}
						</button>
					</div>
	            </div>
	        </div>
	    </div>
	</div>
    <!-- /Modal FB Connexion -->

</div>
<!-- /.container -->
<div class="row clearfix"style="margin-top:20px"></div>
<!--Footer-->

<footer class="footer">
    <!--Footer Links-->
    <div class="container-fluid text-center">
      <!--div class="footer-block text-center logo_footer">
          <a href="{{ domain_url }}?utm=from_logo"><img src="{{ base_url }}/creation-test/src/img/funizi.png" height="30"/></a>
      </div>
      <!--/.Footer Links-->
      <!--Copyright-->
      <div class="footer-block text-center">
          <a href="{{ path_for('privacypolicy') }}" class="link_footer">{{interface_ui.footer_privacy_policy}}</a>
          <!--<a href="#" class="link_footer">{{interface_ui.footer_about_us}}</a>-->
          <a href="https://docs.google.com/forms/d/e/1FAIpQLSfIXQZF0ZmdvjuvIM5bz5pomT-Ds6HH28ppkFR29bVksONofw/viewform" target="_blank" class="link_footer">{{interface_ui.footer_contact_us}}</a>
      </div>
    </div>
    <!--/.Copyright-->
</footer>
<!--/.Footer-->
